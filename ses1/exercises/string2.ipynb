    "def not_bad(s):\n",
    "  not_index = s.find('not')\n",
    "  bad_index = s.find('bad')\n",
    "    \n",
    "  if bad_index > not_index:\n",
    "    return s[:not_index] + 'good' + s[bad_index + 3:]\n",
    "  else:\n",
    "    return s"
