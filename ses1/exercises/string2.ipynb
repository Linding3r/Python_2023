    "def split_half(s):\n",
    "        middle = len(s) // 2\n",
    "        front = s[:middle + len(s) % 2]\n",
    "        back = s[middle + len(s) % 2:]\n",
    "        return front, back\n",
    "\n",
    "def front_back(a, b):\n",
    "    a_front, a_back = split_half(a)\n",
    "    b_front, b_back = split_half(b)\n",
    "    \n",
    "    result = a_front + b_front + a_back + b_back\n",
    "    return result"
   ]
